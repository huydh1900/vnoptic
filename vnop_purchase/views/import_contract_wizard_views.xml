<odoo>
    <record id="view_import_contract_remaining_wizard_form" model="ir.ui.view">
        <field name="name">import.contract.remaining.wizard.form</field>
        <field name="model">import.contract.remaining.wizard</field>
        <field name="arch" type="xml">
            <form string="Nạp phần còn thiếu từ PO">
                <group>
                    <field name="contract_id" readonly="1"/>
                    <field name="source_contract_id"/>
                </group>
                <field name="line_ids">
                    <list editable="bottom">
                        <field name="purchase_id"/>
                        <field name="purchase_line_id"/>
                        <field name="product_id"/>
                        <field name="remaining_qty" readonly="1"/>
                        <field name="quantity"/>
                        <field name="uom_id" readonly="1"/>
                    </list>
                </field>
                <footer>
                    <button name="action_apply" type="object" class="btn-primary" string="Áp dụng"/>
                    <button special="cancel" string="Hủy"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
