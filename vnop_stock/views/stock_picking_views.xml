<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="vnop_view_picking_form_inherit" model="ir.ui.view">
        <field name="name">vnop.view.picking.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='do_print_picking']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//button[@name='%(stock.act_stock_return_picking)d']" position="attributes">
                <attribute name="invisible">state != 'done'</attribute>
            </xpath>
            <xpath expr="//button[@name='action_cancel']" position="attributes">
                <attribute name="class">btn-danger</attribute>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/list" position="attributes">
                <attribute name="create">false</attribute>
                <attribute name="delete">false</attribute>
            </xpath>
        </field>
    </record>

</odoo>